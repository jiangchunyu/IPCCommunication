<manifest xmlns:android="http://schemas.android.com/apk/res/android"

          package="com.chunyu.ipclibrary"
>

    <application android:allowBackup="true"
                 android:label="@string/app_name"
                 android:supportsRtl="true"

    >
        <service android:name="com.chunyu.service.IPCService"
                 android:enabled="true"
                 android:exported="true">
            <intent-filter>
                <action android:name="com.chunyu.service.IPCService"></action>
            </intent-filter>
        </service>
        <service android:name="com.chunyu.service.SimpleService"
                 android:enabled="true"
                 android:exported="true">
            <intent-filter>
                <action android:name="com.chunyu.service.SimpleService"></action>
            </intent-filter>
        </service>
        <service android:name="com.chunyu.service.SimpleMsgService"
                 android:enabled="true"
                 android:exported="true">
            <intent-filter>
                <action android:name="com.chunyu.service.SimpleMsgService"></action>
            </intent-filter>
        </service>
    </application>

</manifest>
